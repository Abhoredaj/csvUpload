<!-- Include the custom CSS file for CSV page -->
<link rel="stylesheet" href="/css/csv.css">

<!-- Container for displaying CSV data -->
<div class="container mt-4 mb-5 pb-5">
    <h3>CSV Data</h3>
    <table id="example" class="table table-striped table-bordered">
        <thead class="bg-light-blue"> 
            <tr>
                <!-- Generate table headers dynamically based on csvHeaders -->
                <% csvHeaders.forEach((header) => { %>
                    <th>
                        <%= header %>
                    </th>
                <% }); %>
            </tr>
        </thead>
        <tbody id="tableBody">
            <% csvData.forEach((row) => { %>
                <tr>
                    <!-- Generate table cells dynamically based on row data -->
                    <% csvHeaders.forEach((header) => { %>
                        <td><%= row[header] %></td>
                    <% }); %>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>

<!-- Include the custom JavaScript file for CSV page -->
<script src="/js/csv.js"></script>
